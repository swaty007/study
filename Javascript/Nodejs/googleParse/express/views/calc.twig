{% extends 'layout.twig' %}

{% block body %}
    <h1>{{title}}</h1>
    <p>Welcome to {{title}}</p>
    <ul id="myULContainer">

    </ul>
    <div class="table-responsive">
        <table id="table_id" class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>Domain</th>
                <th>Position</th>
                <th>query</th>
                <th>titleGoogle</th>
                <th>meta</th>
                <th>href</th>
            </tr>
            </thead>
            <tbody>
            {% for key,items in data %}

                {% for item in items %}
                    <tr>
                        <td>{{ key }}</td>
                        <td>{{ item.position }}</td>
                        <td>{{ item.query }}</td>
                        <td>{{ item.title }}</td>
                        <td>
                            {% for metaKey,meta in item.meta %}
                                <p>
                                    <strong>{{ metaKey }}:</strong>
                                    <span>{{ meta }}</span>
                                </p>
                            {% endfor %}
                        </td>
                        <td>{{ item.href }}</td>
                    </tr>
                {% endfor %}
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
